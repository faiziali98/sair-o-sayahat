@model IEnumerable<SAIR_O_SIYAHAT_2.Models.Admin.SpalshScreen>
<div class="modal fade " id="popup_model" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content" style="border-radius: 15px;top: 100px;">

            <div class="modal-body" style="padding:0;">
                @foreach (var item in Model.Take(1))
                {
                    <img src="@item.Thumbnail" style="width: 100%;height: 293px;    border-radius: 15px;    height: 371px;">
                }
            </div>
        </div>
    </div>
</div>

