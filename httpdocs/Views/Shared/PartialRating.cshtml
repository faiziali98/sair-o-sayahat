    <i style="color:#faca51;" class="fas fa-star "></i>
    <i style="color:grey;" class="fas fa-star "></i>
    <i style="color:grey;" class="fas fa-star "></i>
}
else if (ViewBag.Rating == 4)
{
    <i style="color:#faca51;" class="fas fa-star "></i>
    <i style="color:#faca51;" class="fas fa-star "></i>
    <i style="color:#faca51;" class="fas fa-star "></i>
    <i style="color:#faca51;" class="fas fa-star "></i>
    <i style="color:grey;" class="fas fa-star "></i>
}
else if (ViewBag.Rating == 5)
{
    <i style="color:#faca51;" class="fas fa-star "></i>
    <i style="color:#faca51;" class="fas fa-star "></i>
    <i style="color:#faca51;" class="fas fa-star "></i>
    <i style="color:#faca51;" class="fas fa-star "></i>
    <i style="color:#faca51;" class="fas fa-star "></i>
}