@model IEnumerable<SAIR_O_SIYAHAT_2.Models.ApplicationUser>

<style>
    .active1 {
        background: #2a6c29;
        border-color: #2a6c29;
        color: #fff;
    }
</style>
@if (Model.Count() > 0)
{
    foreach (var sub in Model)
    {
        if (ViewBag.VendorId == sub.Id)
        {
            <div class="">
                <a style="font-size: 14px;font-weight: 600" class="list-group-item list-group-item-action active1" href="/Home/Shop?vendorId=@sub.Id&&filter=4" aria-expanded="false" aria-controls=" sub-men1">
                    <img style="width: 27px;border-radius: 13px;" src="@sub.ImageUrl" />  @sub.UserName
                </a>
            </div>
        }
        else
        {
            <div class="">
                <a style="font-size: 14px;font-weight: 600;color: #00b4d8;" class="list-group-item list-group-item-action" href="/Home/Shop?vendorId=@sub.Id&&filter=4" aria-expanded="false" aria-controls=" sub-men1">
                    <img style="width: 27px;border-radius: 13px;" src="@sub.ImageUrl" /> @sub.UserName
                </a>
            </div>
        }
    }
}
